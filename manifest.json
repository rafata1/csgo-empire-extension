{
    "manifest_version": 3,
    "name": "dungEmpire",
    "description": "This is my first extension",
    "version": "1.0",
    "icons": {
        "16": "/images/16x16.png",
        "48": "/images/48x48.png",
        "128": "/images/128x128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://csgoempire.com/*"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "storage",
        "activeTab"
    ]
}